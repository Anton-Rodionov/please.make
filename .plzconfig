[please]
version = 14.4.2

[buildconfig]
### go
go-vendor-deps = //example_go:vendor
go-protoc-version = 3.11.4
go-protoc-gen-go-version = v1.4.1
go-junit-report-version = v0.9.1

### python
python-vendor-deps = //example_python:vendor

### web
web-vendor-deps = //example_web:vendor
web-toolchain = //example_web/toolchain:default
web-yarn-version = 1.22.4

### rust
rust-workspace = //example_rust:workspace
rust-rq-version = v1.0.2
rust-jq-version = 1.6
rust-cargo-2-junit-version = 0.1.5

### dotnet
dotnet-trx-2-junit-version = 1.3.0

### docker
docker-repository = docker.pkg.github.com/simplebuild/please.make
